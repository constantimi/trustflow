{
  "name": "electron-vite",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "scripts": {
    "dev": "cross-env vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "electron": "electron .",
    "start": "npm run dev",
    "dev-electron": "concurrently \"npm run dev\" \"npm run electron\"",
    "preview": "vite preview",
    "jest": "jest",
    "test": "jest --detectOpenHandles",
    "test:silent": "jest --detectOpenHandles --reporters jest-silent-reporter",
    "test:cov": "jest --coverage",
    "test:cov:export": "jest --coverage --runInBand --detectOpenHandles --ci --testResultsProcessor=jest-junit  --coverageReporters=cobertura --coverageReporters=html --coverageReporters=text --coverageThreshold '{}'",
    "test:cov:export:fail": "jest --coverage --runInBand --detectOpenHandles --ci --testResultsProcessor=jest-junit  --coverageReporters=cobertura --coverageReporters=html --coverageReporters=text",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write --plugin-search-dir=. --plugin=. -- '*.js' '*.jsx' '*.ts' '*.tsx'",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.2",
    "electron": "^24.8.8",
    "i18next": "^23.10.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mantine-react-table": "^1.3.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.4",
    "react-router": "^6.22.2",
    "react-router-dom": "^6.22.2",
    "react-select": "^5.8.0",
    "react-toastify": "^10.0.4",
    "redux": "^4.2.0",
    "uuid": "^9.0.1",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-svgr": "^3.2.0",
    "wait-on": "^7.0.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/electron": "^1.6.10",
    "@types/i18next-sprintf-postprocessor": "^0.2.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
    "npm": "^10.9.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.5"
  }
}
